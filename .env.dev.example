# Development environment variables
EXPO_PUBLIC_ENV=development

# TEMPORARY: Hardcoded wallet for testing while passkeys are being debugged
# TODO: Remove this when passkeys are working on Android
EXPO_PUBLIC_HARDCODED_USER_WALLET=

# RPC URL (get from Alchemy, Infura, etc.)
EXPO_PUBLIC_RPC_URL=

# Chain ID (11155111 for Ethereum Sepolia, 1 for Ethereum Mainnet)
EXPO_PUBLIC_CHAIN_ID=11155111

# Factory contract address
EXPO_PUBLIC_FACTORY_CONTRACT_ADDRESS=0xDD0f9cB4Cf53d28b976C13e7ee4a169F841924c0 # <- Ethereum Sepolia Factory address

# COIL Token contract address
EXPO_PUBLIC_TOKEN_ADDRESS=0x52BF349eAB9DACb0AC8C5980a4C4038C2751304a

# Relayer private key for contract transactions pre user wallet deployment
# Make sure it's funded with Sepolia ETH
# Format should be 0x....
EXPO_PUBLIC_RELAYER_PRIVATE_KEY= 

# Account abstraction bundler & paymaster
EXPO_PUBLIC_PIMLICO_API_KEY=

# Privacy Proxy Configuration (runs on associated-domain-server:8080)
# Use your ngrok domain (same as EXPO_PUBLIC_ASSOCIATED_DOMAIN)
EXPO_PUBLIC_PRIVACY_PROXY_BUNDLER_URL=https://YOUR_NGROK_DOMAIN/rpc/bundler
EXPO_PUBLIC_PRIVACY_PROXY_PAYMASTER_URL=https://YOUR_NGROK_DOMAIN/rpc/paymaster
EXPO_PUBLIC_ENABLE_PRIVACY_MODE=true

# WebAuthn configuration
# iOS Team ID - extract from: grep "DEVELOPMENT_TEAM" ios/*.xcodeproj/project.pbxproj (after running 'npm run prebuild')
EXPO_PUBLIC_IOS_TEAM_ID=
EXPO_PUBLIC_BUNDLE_ID=com.anonymous.anonWallet
# Your ngrok domain - get this from ngrok dashboard, see readme
EXPO_PUBLIC_ASSOCIATED_DOMAIN=
# Android SHA-256 certificate fingerprint - extract from your keystore (see README for instructions)
EXPO_PUBLIC_ANDROID_SHA256_FINGERPRINT= 
